generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  name     String
  password String
  lists    List[]

  @@map("users")
}

model List {
  listID String @id @default(uuid())
  name   String @unique

  user   User   @relation(fields: [userID], references: [id])
  userID String

  @@map("lists")
}
